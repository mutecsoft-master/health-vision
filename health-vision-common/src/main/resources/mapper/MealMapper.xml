<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.mutecsoft.healthvision.common.mapper.MealMapper">

    <!--
        MEAL 관련 공통 조회 컬럼
    -->
    <sql id="mealColumn">
        MEAL_ID
        , MEAL_INFO
        , MEAL_DT
        , USER_ID
        , REG_DT
    </sql>
    
    
    <insert id="insertMeal" parameterType="com.mutecsoft.healthvision.common.model.Meal">
        INSERT INTO TB_MEAL (
            MEAL_INFO,
            MEAL_DT,
            USER_ID,
            REG_DT
        ) VALUES (
            #{mealInfo},
            #{mealDt},
            #{regId},
            NOW()
        )
    </insert>
    
</mapper>